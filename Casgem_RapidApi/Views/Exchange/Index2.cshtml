@model List<ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;


}

<form method="get">
   
    <label>Dövic kuru kodu</label>
    <input type="text" name="x" />
    <button class="btn btn-warning">Getir</button>
    <div class="container">
        <br /> <br />
        <table class="tabl table-hover">
            <tr>
                <th>Dövüz</th>
                <th>miktar</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.currency</td>
                    <td>@item.exchange_rate_buy</td>
                </tr>
            }
        </table>
    </div>
</form>
